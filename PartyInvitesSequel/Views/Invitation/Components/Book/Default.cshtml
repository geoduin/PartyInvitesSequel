@{
}
@model List<Guest>

@for (int i = 0; i < Model.Count; i++)
{
            <tr>
                <td>@Model[i].Name</td>
                <td>@Model[i].Email</td>
                <td>@Model[i].Phone</td>
                <td><a asp-action="Profile" asp-controller="Profile" asp-route-id="i">Ga verder naar pagina</a></td>
            </tr>
        }