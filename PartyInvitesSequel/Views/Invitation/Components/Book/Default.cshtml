@{
}
@model List<Guest>

@for (int i = 0; i < Model.Count; i++)
{
            <tr>
                <td>@Model[i].Name</td>
                <td>@Model[i].Email</td>
                <td>@Model[i].Phone</td>
                <td><a asp-action="Form_Party" asp-controller="Profile" asp-route-index=i>Ga verder naar pagina</a></td>
                <td><a asp-action="DeletionOfGuest" asp-controller="Profile" asp-route-index=i>Verwijder</a></td>
            </tr>
        }