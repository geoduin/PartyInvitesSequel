@{
}
@model GuestList

@for (int i = 0; i < GuestList.Instance().list.Count; i++)
{
            <tr>
        <td>@GuestList.Instance().list[i].Name</td>
        <td>@GuestList.Instance().list[i].Email</td>
        <td>@GuestList.Instance().list[i].Phone</td>
                <td><a asp-action="Profile" asp-controller="Profile" asp-route-id="i">Ga verder naar pagina</a></td>
            </tr>
        }